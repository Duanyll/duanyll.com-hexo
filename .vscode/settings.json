{
  "markdown.extension.katex.macros": {
    "\\d": "\\mathrm{d}",
    "\\p": "\\partial",
    "\\label": "\\boxed{\\textrm{#1}}",
    "\\ref": "\\textrm{#1}",
    "\\un": "\\,\\mathrm{#1}",
    "\\re": "\\operatorname{Re}",
    "\\im": "\\operatorname{Im}",
    "\\conj": "\\overline{#1}",
    "\\Arg": "\\operatorname{Arg}",
    "\\Ln": "\\operatorname{Ln}",
    "\\Res": "\\operatorname*{Res}",
    "\\Cov": "\\operatorname{Cov}",
    "\\st": "\\mathrm{s.t.}",
    "\\ord": "\\operatorname{ord}"
  },
  "markdown.styles": [
    ".vscode/cobalt.css"
  ]
}